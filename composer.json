	"type": "library",
	"keywords": [
		"irc",
		"stats"
	],
	"homepage": "https://github.com/tomzx/irc-stats",
	"license": "MIT",
	"authors": [
		{
			"name": "Tom Rochette",
			"email": "tom@tomrochette.com",
			"homepage": "http://www.tomrochette.com"
		}
	],
	"support": {
